{
  "name": "@kiwiproject/elucidation-client-js",
  "version": "0.1.0",
  "description": "Client code to assist in sending connection events to the elucidation server",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "format": "npx prettier --ignore-path .gitignore --write \"**/*.+(ts|js|json)\"",
    "lint": "npx eslint --fix src",
    "test": "npx jest --coverage --collectCoverageFrom=src/**/*.ts"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/__utils__",
      "<rootDir>/node_modules/"
    ],
    "coveragePathIgnorePatterns": [
      "src/index.ts"
    ]
  },
  "dependencies": {
    "axios": "1.11.0"
  },
  "devDependencies": {
    "@babel/core": "7.28.3",
    "@babel/preset-env": "7.28.3",
    "@babel/preset-typescript": "7.27.1",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.33.0",
    "@jest/globals": "30.0.5",
    "@types/node": "24.3.0",
    "babel-jest": "30.0.5",
    "eslint": "9.33.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "globals": "16.3.0",
    "jest": "30.0.5",
    "prettier": "3.6.2",
    "typescript": "5.9.2",
    "typescript-eslint": "8.39.1"
  }
}
