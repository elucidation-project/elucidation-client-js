{
  "name": "@kiwiproject/elucidation-client-js",
  "version": "0.1.0",
  "description": "Client code to assist in sending connection events to the elucidation server",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "format": "npx prettier --ignore-path .gitignore --write \"**/*.+(ts|js|json)\"",
    "lint": "npx eslint --fix src",
    "test": "npx jest --coverage --collectCoverageFrom=src/**/*.ts"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/__utils__",
      "<rootDir>/node_modules/"
    ],
    "coveragePathIgnorePatterns": [
      "src/index.ts"
    ]
  },
  "dependencies": {
    "axios": "1.8.1"
  },
  "devDependencies": {
    "@babel/core": "7.26.9",
    "@babel/preset-env": "7.26.9",
    "@babel/preset-typescript": "7.26.0",
    "@eslint/eslintrc": "3.3.0",
    "@eslint/js": "9.22.0",
    "@jest/globals": "29.7.0",
    "@types/node": "22.13.10",
    "babel-jest": "29.7.0",
    "eslint": "9.22.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-prettier": "5.2.3",
    "globals": "16.0.0",
    "jest": "29.7.0",
    "prettier": "3.5.3",
    "typescript": "5.8.2",
    "typescript-eslint": "8.26.0"
  }
}
